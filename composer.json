{
    "name":         "ocramius/version-eye-module",
    "description":  "VersionEye Module for Zend Framework 2",
    "type":         "library",
    "license":      "MIT",
    "keywords":     ["zf2", "version eye", "module"],
    "homepage":     "https://github.com/Ocramius/VersionEyeModule",
    "authors":      [
        {
            "name":     "Marco Pivetta",
            "email":    "ocramius@gmail.com",
            "homepage": "http://ocramius.github.com/"
        }
    ],
    "require":      {
        "php":                                  ">=5.4",
        "zendframework/zend-mvc":               "~2.3",
        "zendframework/zend-http":              "~2.3",
        "digitalkaoz/versioneye-php":           "~0.10"
    },
    "require-dev":  {
        "zendframework/zend-developer-tools":   "dev-master",
        "squizlabs/php_codesniffer":            "~1",
        "zendframework/zend-view":              "~2.3",
        "zendframework/zend-cache":             "~2.3",
        "phpunit/phpunit": "~4.2.0"
    },
    "autoload":     {
        "psr-0": {
            "VersionEyeModule\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
